# Environment Configuration
NODE_ENV=development

# AWS Configuration
AWS_REGION=us-east-1
AWS_ACCESS_KEY_ID=your_access_key
AWS_SECRET_ACCESS_KEY=your_secret_key
AWS_KMS_KEY_ID=your_kms_key_id

# Database Configuration (TimescaleDB/PostgreSQL)
DATABASE_HOST=localhost
DATABASE_PORT=5432
DATABASE_NAME=biomedical_platform
DATABASE_USER=postgres
DATABASE_PASSWORD=your_db_password
DATABASE_SSL=true
DATABASE_MAX_CONNECTIONS=100

# Redis Configuration
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=your_redis_password
REDIS_TLS=true

# S3 Storage
S3_BUCKET_IMAGING=biomedical-imaging-data
S3_BUCKET_FEATURES=biomedical-features
S3_BUCKET_MODELS=biomedical-models
S3_BUCKET_EXPORTS=biomedical-exports

# EKS Configuration
EKS_CLUSTER_NAME=biomedical-platform-cluster
EKS_NAMESPACE=biomedical

# Encryption Keys
ENCRYPTION_KEY_AES256=your_256_bit_encryption_key
JWT_SECRET=your_jwt_secret_key
JWT_EXPIRY=24h

# HIPAA Compliance
HIPAA_AUDIT_LOG_BUCKET=biomedical-audit-logs
HIPAA_BACKUP_BUCKET=biomedical-backups
HIPAA_LOG_RETENTION_DAYS=2190
BAA_REQUIRED=true

# AI/ML Model Configuration
ML_MODEL_REGISTRY_URL=http://localhost:8080
KSERVE_INFERENCE_URL=http://kserve-inference.biomedical.svc.cluster.local
TENSORFLOW_SERVING_URL=http://tensorflow-serving:8501

# Medical Imaging AI
DICOM_PACS_URL=http://pacs.hospital.local
DICOM_AE_TITLE=BIOMEDICAL_AI
ORTHANC_URL=http://orthanc:8042
ORTHANC_USERNAME=orthanc
ORTHANC_PASSWORD=orthanc_password

# Biosensing
IOT_CORE_ENDPOINT=your_iot_core_endpoint.amazonaws.com
IOT_CERTIFICATE_PATH=./certs/iot-certificate.pem
IOT_PRIVATE_KEY_PATH=./certs/iot-private-key.pem

# FHIR/HL7 Integration
FHIR_SERVER_URL=http://fhir.hospital.local/fhir
HL7_MLLP_HOST=hl7.hospital.local
HL7_MLLP_PORT=2575

# Monitoring & Logging
PROMETHEUS_URL=http://prometheus:9090
GRAFANA_URL=http://grafana:3000
CLOUDTRAIL_LOG_GROUP=biomedical-platform-logs
ATHENA_DATABASE=biomedical_audit_db

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100

# CORS Origins
CORS_ORIGINS=http://localhost:3000,http://localhost:3001,https://biomedical.myengineering.com

# Feature Flags
ENABLE_FDA_MODE=true
ENABLE_EXPLAINABILITY=true
ENABLE_AGENTIC_TRIAGE=true
ENABLE_DRUG_DISCOVERY=true
ENABLE_GENOMICS=true

# Notification Services
TWILIO_ACCOUNT_SID=your_twilio_account_sid
TWILIO_AUTH_TOKEN=your_twilio_auth_token
TWILIO_PHONE_NUMBER=+1234567890
SENDGRID_API_KEY=your_sendgrid_api_key
SENDGRID_FROM_EMAIL=noreply@biomedical.myengineering.com

# WebSocket Configuration
WEBSOCKET_PORT=8080
WEBSOCKET_PATH=/ws

# Ports for Each Service
PORT_AI_DIAGNOSTICS=5001
PORT_MEDICAL_IMAGING=5002
PORT_BIOSENSING=5003
PORT_HIPAA_COMPLIANCE=5004
PORT_BIOTENSOR_LABS=5005
PORT_MYNX_NATALCARE=5006

# Frontend URLs
FRONTEND_AI_DIAGNOSTICS=http://localhost:3001
FRONTEND_MEDICAL_IMAGING=http://localhost:3002
FRONTEND_BIOSENSING=http://localhost:3003
FRONTEND_HIPAA_COMPLIANCE=http://localhost:3004
FRONTEND_BIOTENSOR_LABS=http://localhost:3005
FRONTEND_MYNX_NATALCARE=http://localhost:3006
