# Medical Imaging AI Frontend - Environment Variables

# API Configuration
NEXT_PUBLIC_API_URL=http://localhost:5002

# Application Settings
NEXT_PUBLIC_APP_NAME=Medical Imaging AI
NEXT_PUBLIC_APP_VERSION=1.0.0

# Feature Flags
NEXT_PUBLIC_ENABLE_GRADCAM=true
NEXT_PUBLIC_ENABLE_ORTHANC=true
NEXT_PUBLIC_ENABLE_ADVANCED_TOOLS=true

# Cornerstone Configuration
NEXT_PUBLIC_MAX_WEB_WORKERS=4
NEXT_PUBLIC_ENABLE_WEBGL=true

# Development
NEXT_PUBLIC_DEBUG_MODE=false
